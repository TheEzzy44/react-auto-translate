{
  "name": "gbusiness-ai-react-auto-translate",
  "version": "1.1.1",
  "description": "Simple React components to automatically translate strings with Google Translate API",
  "scripts": {
    "build": "bob build",
    "prepublish": "npm run build",
    "lint": "tsc --noEmit && npx eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "prepare": "bob build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "react",
    "react",
    "native",
    "i18tn",
    "translate"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-teamgantt": "^2.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-native-builder-bob": "^0.35.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/example/",
      "/lib/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "peerDependencies": {
    "react": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teamgantt/react-auto-translate.git"
  },
  "main": "lib/commonjs/index.js",
  "react-native": "src/index.ts",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "gbusiness-ai-react-auto-translate": "^1.0.0"
  }
}
